<script>
  let year21 = document.querySelector('.yearly21-modal');
  let year20 = document.querySelector('.yearly20-modal');
  let year19 = document.querySelector('.yearly19-modal');
  let bg = document.querySelectorAll('.yearly-modal .bg');
  function modalOpen(v) {
    if(v === '21') {
      year21.classList.remove('hidden');
    } else if( v === '20') {
      year20.classList.remove('hidden');
    } else {
      year19.classList.remove('hidden');
    }
  }

  let closeBtn = document.querySelectorAll('.yearly-modal .closeBtn');
  Array.prototype.forEach.call(closeBtn, function(e, idx) {
    e.addEventListener('click', function() {
      if(idx === 0) {
        year21.classList.add('hidden');
      } else if(idx === 1) {
        year20.classList.add('hidden');
      } else {
        year19.classList.add('hidden');
      }
    })
  });

  Array.prototype.forEach.call(bg, function(e) {
    e.addEventListener('click', function() {
      year21.classList.add('hidden');
      year20.classList.add('hidden');
      year19.classList.add('hidden');
    });
  });
</script>



